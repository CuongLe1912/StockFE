{
    "entity": {
        "import": {
            "base": "import { BaseEntity } from '../../../../_core/domains/entities/base.entity';",
            "table": "import { TableDecorator } from '../../../../_core/decorators/table.decorator';",
            "dropdown": "import { DropDownDecorator } from '../../../../_core/decorators/dropdown.decorator';",
            "number": "import { NumberDecorator } from '../../../../_core/decorators/number.decorator';",
            "string": "import { StringDecorator } from '../../../../_core/decorators/string.decorator';",
            "boolean": "import { BooleanDecorator } from '../../../../_core/decorators/boolean.decorator';",
            "datetime": "import { DateTimeDecorator } from '../../../../_core/decorators/datetime.decorator';",
            "file": "import { FileDecorator } from '../../../../_core/decorators/file.decorator';",
            "image": "import { ImageDecorator } from '../../../../_core/decorators/image.decorator';",
            "video": "import { VideoDecorator } from '../../../../_core/decorators/video.decorator';",

            "type": {
                "allString": "import { {{TypeImport}} } from '../../../../_core/domains/enums/data.type';",
                "string": "StringType",
                "number": "NumberType",
                "boolean": "BooleanType",
                "datetime": "DateTimeType"
            }
        }
    },

    "module" : [
        "import { NgModule } from '@angular/core';",
        "import { CommonModule } from '@angular/common';",
        "import { RouterModule } from '@angular/router';",
        "import { UtilityModule } from '../../../modules/utility.module';",        
        "import { AdminAuthGuard } from '../../../_core/guards/admin.auth.guard';",
        "import { {{ModuleName}}Component } from './{{ModuleNameLowerCase}}.component';",
        "",
        "@NgModule({",
        "  imports: [",
        "    CommonModule,",
        "    UtilityModule,",
        "    RouterModule.forChild([",
        "      { path: '', component: {{ModuleName}}Component, pathMatch: 'full', data: { state: '{{ModuleNameLowerCase}}' }, canActivate: [AdminAuthGuard] },",
        "    ]),",
        "  ],",
        "  declarations: [{{ModuleName}}Component]",
        "})",
        "export class {{ModuleName}}Module { }"
    ],

    "component": [
        "import { Component } from '@angular/core';",
        "import { GridData } from '../../../_core/domains/data/grid.data';",
        "import { ActionData } from '../../../_core/domains/data/action.data';",
        "import { ModalSizeType } from '../../../_core/domains/enums/modal.size.type';",
        "import { GridComponent } from '../../../_core/components/grid/grid.component';",
        "import { ActionType } from '../../../_core/domains/enums/action.type';",
        "import { {{ModuleName}}Entity } from './entities/{{ModuleNameLowerCase}}.entity';",
        "",
        "@Component({",
        "  templateUrl: '../../../_core/components/grid/grid.component.html',",
        "})",
        "export class {{ModuleName}}Component extends GridComponent {",
        "  obj: GridData = {",
        "    Reference: {{ModuleName}}Entity,",
        "    Size: ModalSizeType.Large,",
        "    UpdatedBy: false,",
        "    Imports: [],",
        "    Exports: [],",
        "    Filters: [],",
        "    Features: [",
        "      ActionData.reload(() => {",
        "        this.loadItems();",
        "      }),",
        "    ],",
        "    Actions: [",
        "      {",
        "        icon: 'la la-eye',",
        "        name: ActionType.View,",
        "        className: 'btn btn-warning',",
        "        systemName: ActionType.ViewDetail,",
        "        click: (item: any) => this.view(item)",
        "      },",
        "    ],",
        "    DisableAutoLoad: true,",
        "  };",
        "",
        "  constructor() {",
        "    super();",
        "  }",
        "",
        "  async ngOnInit() {",
        "    this.render(this.obj);",
        "  }",
        "}"
    ]
}